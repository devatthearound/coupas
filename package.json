{
  "name": "coupas",
  "version": "1.0.11",
  "private": true,
  "homepage": "https://growsome.kr",
  "author": {
    "name": "devatthearound",
    "email": "dev@the-around.com",
    "url": "https://the-around.com"
  },
  "main": "build/index.js",
  "description": "NextJS App with Electron",
  "scripts": {
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "next:lint": "next lint",
    "format": "dprint fmt",
    "postinstall": "electron-builder install-app-deps",
    "electron:dist": "electron-builder",
    "electron:build": "tsup",
    "build": "run-s next:build electron:build",
    "dist": "run-s build electron:dist",
    "publish": "run-s build electron:publish",
    "electron:publish": "electron-builder --publish always",
    "dev": "npm-run-all --parallel electron:dev next:dev",
    "electron:build_watch": "tsup --watch",
    "electron:dev": "npm-run-all --parallel electron:build_watch electron:watch",
    "electron:watch": "cross-env NODE_ENV='development' nodemon",
    "test:image": "cross-env NODE_ENV='development' node --loader ts-node/esm electron/main/test.ts"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^3.0.0",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@next/env": "^14.2.5",
    "axios": "^1.8.1",
    "bcrypt": "^6.0.0",
    "cross-env": "^7.0.3",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "electron-notarize": "^1.2.2",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "get-port-please": "^3.1.2",
    "google-auth-library": "^9.15.1",
    "googleapis": "^146.0.0",
    "jose": "^6.0.8",
    "jszip": "^3.10.1",
    "moment": "^2.30.1",
    "next": "15.1.4",
    "node-fetch": "^3.3.2",
    "openai": "^4.86.1",
    "pg": "^8.13.3",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "recharts": "^3.1.0",
    "sharp": "^0.33.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@types/bcrypt": "^5.0.2",
    "@types/ffprobe-static": "^2.0.3",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^22.1.0",
    "@types/pg": "^8.11.11",
    "@types/react": "^18.3.3",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.3.0",
    "@types/recharts": "^1.8.29",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.5.7",
    "eslint-config-next": "14.2.5",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.7",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "build": {
    "appId": "com.growsome.coupas",
    "productName": "coupas",
    "executableName": "coupas",
    "copyright": "Copyright © 2025 The Around",
    "forceCodeSigning": false,
    "asar": true,
    "publish": [
      {
        "provider": "github",
        "owner": "devatthearound",
        "repo": "coupas",
        "private": false,
        "vPrefixedTagName": true,
        "releaseType": "release",
        "publishAutoUpdate": true
      }
    ],
    "asarUnpack": [
      "node_modules/next",
      "node_modules/@img",
      "node_modules/next/dist/compiled/@vercel/og",
      "node_modules/sharp",
      "node_modules/ffmpeg-static",
      "node_modules/ffprobe-static",
      "**\\*.{node,dll}"
    ],
    "extraMetadata": {
      "main": "build/index.js"
    },
    "files": [
      "build",
      {
        "from": ".next/standalone",
        "to": "app",
        "filter": [
          "!**/.env",
          "!**/package.json",
          "!**/md/**"
        ]
      },
      {
        "from": ".next/static",
        "to": "app/.next/static"
      },
      {
        "from": "public",
        "to": "app/public"
      }
    ],
    "extraResources": [
      {
        "from": "electron/assets",
        "to": "assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "artifactName": "coupas-win-${version}-${arch}.${ext}",
      "publisherName": "The Around",
      "verifyUpdateCodeSignature": false,
      "icon": "electron/assets/icon.ico",
      "compression": "store",
      "signIgnore": ["**/*"]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": false,
      "installerIcon": "electron/assets/icon.ico",
      "uninstallerIcon": "electron/assets/icon.ico",
      "installerHeaderIcon": "electron/assets/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "쿠파스",
      "deleteAppDataOnUninstall": false,
      "displayLanguageSelector": false,
      "unicode": true,
      "runAfterFinish": true,
      "menuCategory": "디어라운드"
    },
    "mac": {
      "target": [
        "zip"
      ],
      "artifactName": "coupas-mac-${version}-${arch}.${ext}",
      "category": "public.app-category.productivity",
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "identity": null,
      "signIgnore": ["**/*"]
    },
    "dmg": {
      "icon": "electron/assets/icon.icns",
      "iconSize": 100,
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "format": "ULFO"
    },
    "directories": {
      "output": "releases",
      "buildResources": "electron/assets"
    },
    "protocols": [
      {
        "name": "Coupas Auth",
        "schemes": [
          "coupas-auth"
        ],
        "role": "Viewer"
      },
      {
        "name": "Coupas Youtube",
        "schemes": [
          "google-auth"
        ],
        "role": "Viewer"
      }
    ]
  }
}
